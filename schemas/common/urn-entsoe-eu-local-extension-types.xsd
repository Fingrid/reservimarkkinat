<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:ecl="urn:entsoe.eu:wgedi:codelists" xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xsd:simpleType name="LocalAllocationModeType">
		<xsd:restriction base="xsd:NMTOKEN"/>
	</xsd:simpleType>
	<xsd:simpleType name="LocalAnalogType">
		<xsd:restriction base="xsd:NMTOKEN"/>
	</xsd:simpleType>
	<xsd:simpleType name="LocalAssetType">
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="Z03">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Transformer station</Title>
							<Definition>A resource that can transforms voltage</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z04">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Thermal</Title>
							<Definition>A resource using thermal for energy</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z05">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Wind</Title>
							<Definition>A resource using wind for energy</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z08">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Wave</Title>
							<Definition>A resource using wave for energy</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="LocalAuctionType">
		<xsd:restriction base="xsd:NMTOKEN"/>
	</xsd:simpleType>
	<xsd:simpleType name="LocalBusinessType">
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="Z05">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Net internal trade counterpart</Title>
							<Definition>Net internal trade as reported from the counterpart, used during matching procedure.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z08">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Trade, unconfirmed</Title>
							<Definition>The trade plan of an actor when not matched against a counterpart, used during matching procedure.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z12">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Total primary reserve</Title>
							<Definition>Sum of all primary reserves </Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z13">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Balance regulation activations</Title>
							<Definition>Activations of tertiary reserves in the Balance regulation market</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z14">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>System operator adjustment </Title>
							<Definition>Proposed adjustment by the System operator, used during matching procedure.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z15">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Result of an automatic System operator adjustment </Title>
							<Definition>Forced adjustment by the System operator, used during matching procedure.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z16">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Market schedules difference </Title>
							<Definition>Difference between own and counterparty market schedules, used during matching procedure.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z17">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Technical minimum</Title>
							<Definition>Technical minimum possible, under minimum, Resource Object having the possibility of using overload areas.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z18">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Technical maximum</Title>
							<Definition>Technical maximum possible, maximum incl. overload, Resource Object having the possibility of using overload areas.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z19">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Total maximum production</Title>
							<Definition>Schedule for maximum total production.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z20">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Total minimum production</Title>
							<Definition>Schedule for minimum total production.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z21">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Total Transfer Capacity (TTC)</Title>
							<Definition>The Total Transfer Capacity is the maximum exchange program between two areas compatible with operational security standards applicable at each system if future network conditions, generation and load patterns were perfectly known in advance.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z22">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Maximum production capacity</Title>
							<Definition>Maximum total value of planned production.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z23">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Minimum production capacity</Title>
							<Definition>Minimum total value of planned production</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z24">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Peak load resource</Title>
							<Definition>Peak load resource refers to power plants which produce electricity using condensing power capacity covered by the power load reserve arrangement and may also refer to dis-connectable consumption.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z27">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Operational capacity (OC)</Title>
							<Definition>Operational capacity is exchanged between System operators. The OC is the available transfer capacity as established during the operational day, i.e. the capacity available after closure of the intra-day market. The OC is used for system operation and not for market purposes. The OC may be both higher and lower than the ATC. The OC may be negative. This is a permanent Nordic code.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z28">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Balance regulation power</Title>
							<Definition>Offered tertiary reserves to the Balance regulation market</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z29">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Reserves option power</Title>
							<Definition>Offered tertiary reserves to the Reserves option market</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z32">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>System price</Title>
							<Definition>The time series provides the system price, which is calculated as the price that will be realised if there are no congestions between Market balance areas.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z35">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Commercial bid</Title>
							<Definition>The time series provides commercial bids.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z36">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Reserve bid</Title>
							<Definition>The time series provides reserve bids.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z37">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>The time series provides reserve bids.</Title>
							<Definition>Activations of primary reserves</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z38">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Hydro production</Title>
							<Definition>The business being described concerns production based on hydro power</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z39">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Nuclear production</Title>
							<Definition>The business being described concerns production based on nuclear power</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z40">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Thermal production</Title>
							<Definition>The business being described concerns production based on thermal power</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z41">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Wind production</Title>
							<Definition>The business being described concerns production based on wind power</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z42">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Decentralised production</Title>
							<Definition>The business being described concerns production based on decentralised power</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z43">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Gas turbine and diesel production</Title>
							<Definition>The business being described concerns production based on gas turbine and diesel power</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z44">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Other thermal production</Title>
							<Definition>The business being described concerns production based on other thermal power</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z45">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Disturbance on the Link</Title>
							<Definition>Nordic code (currently used by SwePol)</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z46">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>System Operator redispatching, in case of ASP special</Title>
							<Definition>Nordic code (currently used by SwePol)</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z47">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Loop Flow</Title>
							<Definition>Nordic code (currently used by SwePol)</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z48">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Number of return cables</Title>
							<Definition>Nordic code (currently used by SwePol)</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z49">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Commercial production</Title>
							<Definition>The time series provides commercial bids based on production</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z50">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Commercial wind production</Title>
							<Definition>The time series provides commercial bids based on wind production</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z51">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Commercial consumption</Title>
							<Definition>The time series provides commercial bids based on consumption</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z52">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Small scale production</Title>
							<Definition>Production from small scale production plants</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z64">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Internal trade difference</Title>
							<Definition>A time series concerning internal trade difference, within a Market balance area, i.e. the difference between trades reported from an out party (seller) and an in party (buyer). The internal trade difference is the delta value between what is reported by the two Balance responsible Parties.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z66">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Last resort</Title>
							<Definition>A time series concerning consumption handled by supplier of last resort</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z67">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>TRM</Title>
							<Definition>Transmission Reliability Margin</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z68">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Production Units own consumption</Title>
							<Definition>The consumption of one or more Production Units</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z73">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Fast active disturbance reserve</Title>
							<Definition>The fast active disturbance reserve is the manual reserve (mFRR) available within 15 minutes in the event of the loss of an individual principal component (production unit, line, transformer, bus bar etc.). Restores the frequency controlled disturbance reserve.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z74">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Imbalance sales price</Title>
							<Definition>A time series concerning imbalance prices for sales</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z75">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Imbalance purchase price</Title>
							<Definition>A time series concerning imbalance prices for purchase</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z76">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Day ahead prices used for CZC forecast calculation</Title>
							<Definition>Day ahead prices used for Cross Zonal Capacity forecast calculation.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z77">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>ACE OL (Area Control Error Open Loop)</Title>
							<Definition>The Area Control Error Open Loop (ACE OL) is the real-time imbalance of an area in the power system without automatic Frequency Restoration Reserve (aFRR) and manual Frequency Restoration Reserves (mFRR). ACE OL is the imbalance before any operator balancing actions.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z78">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Upper Alert</Title>
							<Definition>A time series concerning the upper limit before an alarm is raised</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z79">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Upper Emergency</Title>
							<Definition>A time series concerning the upper limit before an emergency is raised</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z80">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Lower Alert</Title>
							<Definition>A time series concerning the lower limit before an alarm is raised</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z81">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Lower Emergency</Title>
							<Definition>A time series concerning the lower limit before an emergency is raised</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z82">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Upper Warning</Title>
							<Definition>A time series concerning the upper limit before a warning is raised</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z83">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Lower Warning</Title>
							<Definition>A time series concerning the lower limit before a warning is raised</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z84">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Inertia</Title>
							<Definition>A time series concerning the ability of the kinetic energy stored in the rotating masses in the electricity system to resist changes in frequency.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z85">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>FFR</Title>
							<Definition>A time series concerning the Fast Frequency Reserve (FFR), a power response activated within about one second when the system frequency drops below a certain level.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z86">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Frequency nadir</Title>
							<Definition>A time series concerning the expected lowest value of the frequency, after a disturbance.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z87">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Reference incident</Title>
							<Definition>A time series concerning the expected maximum incident size in MW.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z88">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Total planned flow</Title>
							<Definition>A time series concerning total planned flow of power between areas.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z89">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Expected countertrade</Title>
							<Definition>The planned supportive power corrections.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z90">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Power Plan Trade</Title>
							<Definition>Sum of planned flow considering the Day ahead and Intraday market.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z91">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Loop transit</Title>
							<Definition>Flow resulting from agreed loop flows and transit agreement.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z92">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Agreed supportive power (ASP)</Title>
							<Definition>Flow from agreed supportive power.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z93">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Production adjustments</Title>
							<Definition>Period shift products activated in the balancing time frame where the activation and need are in different areas.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z94">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Frequency Containment Reserve-Disturbance (FCR-D), one-step activation</Title>
							<Definition>FCR-D one step activation, is a reserve that is automatically activated when the frequency falls below 49.90 Hz after an imbalance, activated in one step, as opposed to a linear activation.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z95">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Non-conform load schedule</Title>
							<Definition>Non-Conform Load Schedule: Loads that do not follow a daily and seasonal load variation pattern.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z96">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Conform load schedule</Title>
							<Definition>Conform Load Schedule: A curve of load versus time (X-axis) showing the active power values (Y1-axis) and reactive power (Y2-axis) for each unit of the period covered. This curve represents a typical pattern of load over the time period for a given day type and season.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z97">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Faster than standard FAT</Title>
							<Definition>Bids that can support a “Full Activation Time” (FAT) that is faster than 12,5 minutes. Fast activation can be done for bids with activation time (CIM:activation_ConstraintDuration.duration) shorter than the minimum requirement for the standard product. When circumstances call for it, the TSO can order activation of such bids on a shorter notice.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
