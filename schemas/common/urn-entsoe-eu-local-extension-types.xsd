				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Short and Medium Term Adequacy Assessment (SMTA)</Title>
							<Definition>Process related to exchange of the short- and medium-term adequacy assessment</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z12">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>ACE OL real-time </Title>
							<Definition>The process of exchanging real-time ACE OL values</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z13">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Corrected real time values</Title>
							<Definition>Reporting of real time values after validation and correction.</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z14">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>Fast Frequency Reserve (FFR) Process</Title>
							<Definition>The process of exchanging information regarding the Fast Frequency Reserve (FFR).</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Z15">
				<xsd:annotation>
					<xsd:documentation>
						<CodeDescription>
							<Title>External trade</Title>
							<Definition>Trade outside the Capacity Calculation Region</Definition>
						</CodeDescription>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
